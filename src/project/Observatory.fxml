            <TableColumn fx:id="obArea" prefWidth="100.0" text="Area" />
        </columns>
      </TableView>
      <Button layoutX="133.0" layoutY="326.0" mnemonicParsing="false" onAction="#loadAddObservatory" text="Add Observatory" />
      <Button layoutX="31.0" layoutY="326.0" mnemonicParsing="false" onAction="#loadMain_Menu" text="Back" />
      <Button layoutX="278.0" layoutY="326.0" mnemonicParsing="false" onAction="#removeOb" text="Remove Observatory" />
      <Button layoutX="449.0" layoutY="326.0" mnemonicParsing="false" onAction="#loadUpdateObservatory" text="Update Observatory" />
   </children>
